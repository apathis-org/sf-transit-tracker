# SF Transit Tracker Environment Variables
# Copy this file to .env and fill in your values

# API Keys
# Get your 511.org API key from: https://511.org/developers/list/tokens/create
SF_511_API_KEY=your_511_api_key_here

# Get your BART API key from: https://www.bart.gov/schedules/developers/api
BART_API_KEY=your_bart_api_key_here

# Flask Configuration
DEBUG=True
SECRET_KEY=your-secret-key-here-change-in-production
PORT=5000

# Optional: Database URL (if you add persistence later)
# DATABASE_URL=sqlite:///transit_tracker.db

# Optional: Redis URL (if you add caching later)  
# REDIS_URL=redis://localhost:6379

# Optional: Logging Level
# LOG_LEVEL=INFO